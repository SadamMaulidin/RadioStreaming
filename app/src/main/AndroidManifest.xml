<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tapperware.radiostreaming">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/rstrem"
        android:label="@string/app_name"
        android:roundIcon="@drawable/rstrem"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"/>
            <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PilihActivity" />
        <activity android:name=".NasionalActivity" />
        <activity android:name=".InternasionalActivity" />
        <activity android:name=".DetailRadio" />

    </application>

</manifest>